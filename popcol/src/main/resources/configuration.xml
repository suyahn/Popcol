<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<properties resource="jdbc.properties" />
	
	<typeAliases>
		<typeAlias alias="booking" type="popcol.model.Booking" />
		<typeAlias alias="customer" type="popcol.model.Customer" />
		<typeAlias alias="event" type="popcol.model.Event" />
		<typeAlias alias="faq" type="popcol.model.Faq" />
		<typeAlias alias="location" type="popcol.model.Location" />
		<typeAlias alias="movie" type="popcol.model.Movie"/>
		<typeAlias alias="notice" type="popcol.model.Notice" />
		<typeAlias alias="price" type="popcol.model.Price" />
		<typeAlias alias="qna" type="popcol.model.Qna" />
		<!-- <typeAlias alias="review" type="popcol.model.Review" /> -->
		<typeAlias alias="runningtimetable" type="popcol.model.RunningtimeTable" />
		<typeAlias alias="theater" type="popcol.model.Theater" />
		<typeAlias alias="mypagebooking" type="popcol.model.MypageBooking" />
	</typeAliases>
	
	<mappers>
		<mapper resource="Booking.xml" />
		<mapper resource="Customer.xml" />
		<mapper resource="Event.xml" />
		<mapper resource="Faq.xml" />
		<mapper resource="Location.xml" />
		<mapper resource="Movie.xml" />
		<mapper resource="Notice.xml" />
		<mapper resource="Price.xml" />
		<mapper resource="Qna.xml" />
		<!-- <mapper resource="Review.xml" /> -->
		<mapper resource="RunningtimeTable.xml" />
		<mapper resource="Theater.xml" />
		<mapper resource="MypageBooking.xml" />
	</mappers>
	
</configuration>